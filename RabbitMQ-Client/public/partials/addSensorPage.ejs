<div id="wrapper">


	<div class="row">
		<div class="col-lg-12">
			<h1 class="page-header">Subscribed Radars</h1>
		</div>
		<!-- /.col-lg-12 -->
	</div>
	<!-- /.row -->
	<div class="row">
		<div class="col-lg-12">
			<div class="panel panel-default">
				<div class="panel-heading">Add Radar</div>
				<!-- /.panel-heading -->
				<div class="panel-body">
					<div class="dataTable_wrapper">
						<table width="100%"
							class="table table-striped table-bordered table-hover"
							id="dataTables-example">
							<thead>
								<tr>
									<th>Radar Name</th>
									<th>Radar Type</th>
									<th>Location</th>
									<th>Manufacturer</th>
									<th>Subscribe</th>
								</tr>
							</thead>
							<tbody>
								<tr class="odd gradeX" ng-repeat="sensor in sensorList">
									<td>{{sensor.sensorname}}</td>
									<td>{{sensor.sensortypealias}}</td>
									<td>{{sensor.location}}</td>
									<td class="center">{{sensor.manufacturer}}</td>
									<td class="center"><button type="submit" class="btn btn-primary-outline" ng-disabled="isDisabled" ng-click="subscribeSensor(sensor)">Subscribe</button></td>
								</tr>
							</tbody>
						</table>
					</div>
					<!-- /.table-responsive -->
				</div>
				<!-- /.panel-body -->
			</div>
			<!-- /.panel -->
		</div>
		<!-- /.col-lg-6 -->
	</div>
	<!-- /.row -->


</div>
<!-- /#wrapper -->