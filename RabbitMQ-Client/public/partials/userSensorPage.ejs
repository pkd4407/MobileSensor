<div id="wrapper" ng-hide="hideSensorList">
	<div class="row">
		<div class="col-lg-12">
			<h1 class="page-header">My Radars</h1>
		</div>
		<!-- /.col-lg-12 -->
	</div>
	<!-- /.row -->
	<div class="row">
		<div class="col-lg-12">
			<div class="panel panel-default">
				<div class="panel-heading">Your radar configuration</div>
				<!-- /.panel-heading -->
				<div class="panel-body">
					<div class="dataTable_wrapper">
						<table width="100%"
							class="table table-striped table-bordered table-hover"
							id="dataTables-example">
							<thead>
								<tr>
									<th>Radar Name</th>
									<th>Radar Type</th>
									<th>Location</th>
									<th>Manufacturer</th>
								</tr>
							</thead>
							<tbody>
								<tr class="odd gradeX" ng-repeat="sensor in usersensorList">
									<td  style="cursor: pointer;" ng-click="getSensorData(sensor)"><a>{{sensor.sensorname}}</a></td>
									<td>{{sensor.sensortypealias}}</td>
									<td>{{sensor.location}}</td>
									<td class="center">{{sensor.manufacturer}}</td>
								</tr>
							</tbody>
						</table>
					</div>
					<!-- /.table-responsive -->
				</div>
				<!-- /.panel-body -->
			</div>
			<!-- /.panel -->
		</div>
		<!-- /.col-lg-6 -->
	</div>
	<!-- /.row -->
</div>
<!-- /#wrapper -->
<div ng-hide="hideSenorData">
<h2>Mean Value : {{mean}}</h2>
<h2>Median Value : {{median}}</h2>
<h2>Standard Deviation: {{deviation}}</h2>
<iframe ng="no" width="900" height="500" frameborder="0" scrolling="no" ng-src="{{trustSrc(plotly)}}"></iframe>

</div>
